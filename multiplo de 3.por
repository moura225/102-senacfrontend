	funcao inicio()
	{
		escreva("Digite um valor:")
		leia(valor)

		//Verifica se o número é multiplo de 3 e 5 ou ambos
		se(valor%3 ==0)
		escreva("Valor é múltiplo de 3")
		senao
		escreva("Valor não é múltiplo de 3")
		
		
	}
}
